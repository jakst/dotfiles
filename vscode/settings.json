{
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": [
          "support.type",
          "entity.name.type",
          "entity.name.type.interface"
        ],
        "settings": {
          "foreground": "#e4d078d9"
        }
      },
      {
        "scope": ["entity.name.type.class"],
        "settings": {
          "foreground": "#4ec9b0"
        }
      }
    ]
  },
  "emmet.showExpandedAbbreviation": "inMarkupAndStylesheetFilesOnly",
  "editor.codeActionsOnSave": {
    "source.fixAll": true
    // "source.organizeImports": true
  },
  "files.trimTrailingWhitespace": true,
  "editor.tabSize": 2,
  "explorer.confirmDragAndDrop": false,
  "debug.toolBarLocation": "floating",
  "editor.linkedEditing": true,
  "editor.codeLensFontSize": 9,
  "editor.codeLensFontFamily": "arial",
  "editor.stickyTabStops": true,
  "editor.tabCompletion": "on",
  "editor.suggest.localityBonus": true,
  "editor.renderWhitespace": "all",
  "editor.cursorSmoothCaretAnimation": true,
  "editor.smoothScrolling": true,
  "git.autofetch": true,
  "prettier.singleQuote": true,
  "prettier.semi": false,
  "prettier.trailingComma": "es5",
  "prettier.useTabs": false,
  "editor.autoClosingBrackets": "beforeWhitespace",
  "editor.autoClosingQuotes": "beforeWhitespace",
  "html.autoClosingTags": true,
  "eslint.packageManager": "yarn",
  "workbench.tree.indent": 20,
  "editor.suggestSelection": "first",
  "gitlens.codeLens.enabled": false,
  "[javascript][typescript][typescriptreact][json][jsonc][html][markdown][yaml][svg][xml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "search.useGlobalIgnoreFiles": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "terminal.integrated.defaultProfile.osx": "zsh",
  "scm.alwaysShowRepositories": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "workbench.sideBar.location": "right",
  "editor.suggest.showStatusBar": true,
  "typescript.enablePromptUseWorkspaceTsdk": true,
  // "typescript.implementationsCodeLens.enabled": true,
  "typescript.preferences.includePackageJsonAutoImports": "on",
  // "typescript.referencesCodeLens.enabled": true,
  // "typescript.referencesCodeLens.showOnAllFunctions": true,
  "typescript.surveys.enabled": false,
  "ruby.rubocop.executePath": "/Users/jakst/.rbenv/shims/",
  "go.toolsManagement.autoUpdate": true,
  "editor.minimap.enabled": false,
  "editor.formatOnType": true,
  "editor.formatOnSave": true,
  "editor.accessibilitySupport": "off",
  "editor.suggest.preview": true,
  "remoteHub.search.download.enabled": true,
  "redhat.telemetry.enabled": false,
  "js/ts.implicitProjectConfig.strictNullChecks": true,
  "typescript.implementationsCodeLens.enabled": true,
  "typescript.referencesCodeLens.enabled": true,
  "typescript.referencesCodeLens.showOnAllFunctions": true,
  "vscode-edge-devtools.mirrorEdits": true,
  "workbench.colorTheme": "Default Dark+",
  "rust-analyzer.inlayHints.enable": false,
  "editor.unicodeHighlight.ambiguousCharacters": false,
  "editor.unicodeHighlight.invisibleCharacters": false,
  "diffEditor.renderSideBySide": false,
  "explorer.confirmUndo": "verbose",
  "terminal.integrated.env.osx": {
    "FIG_NEW_SESSION": "1"
  },
  "terminal.integrated.autoReplies": {
    "[Oh My Zsh] Would you like to check for updates? [Y/n]": "Y\r"
  },
  "css.validate": false,
  "editor.quickSuggestions": {
    "strings": true
  },
  "explorer.experimental.fileNesting.enabled": false,
  "explorer.experimental.fileNesting.patterns": {
    "*.ts": "$(capture).js, $(capture).d.ts, $(capture).test.ts ",
    "*.js": "$(capture).js.map, $(capture).min.js, $(capture).d.ts, $(capture).test.js",
    "*.jsx": "$(capture).js, $(capture).test.jsx",
    "*.tsx": "$(capture).ts, $(capture).test.tsx",
    "tsconfig.json": "tsconfig.*.json",
    "package.json": "package-lock.json, .npmrc, yarn.lock, .yarnrc"
  },
  "explorer.experimental.fileNesting.expand": false,
  "window.zoomLevel": 1
}
